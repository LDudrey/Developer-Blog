<!-- Loop over each post in the object array -->
{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/post/{{post.id}}">{{post.title}}</a>
      <p>Posted by {{post.username}} on {{format_date post.date_created}}</p>
    </h2>
    <p>
      {{post.content}}
    </p>
  </div>
</div>
{{/each}}